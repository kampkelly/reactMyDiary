    "babel-preset-react": "^6.24.1",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "materialize-css": "^1.0.0-rc.2",
    "medium-editor": "^5.23.3",
    "moment": "^2.22.2",
    "nodemon": "^1.18.7",
    "npm-watch": "^0.4.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-avatar": "^3.4.6",
    "react-dom": "^16.6.0",
    "react-html-parser": "^2.0.2",
    "react-idle-timer": "^4.0.9",
    "react-materialize": "^2.4.8",
    "react-medium-editor": "^1.8.1",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-star-ratings": "^2.3.0",
    "react-tag-input": "^6.0.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "url-loader": "^1.1.2"
