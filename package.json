{
  "name": "reactMyDiary",
  "version": "1.0.0",
  "description": "",
  "main": "Index.js",
  "engines": {
    "node": ">=8.11.4",
    "npm": "5.6.0"
  },
  "watch": {
    "dev-eslint": "src/**/*.js"
  },
  "scripts": {
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "coverallsgg": "jest --coverage && cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "dev-eslint": "npm run eslint && webpack-dev-server --mode development --hot",
    "dev": "webpack-dev-server --mode development --hot --display-error-details",
    "start": "node server.js",
    "build": "webpack --mode production",
    "postinstall": "npm run build",
    "eslint-fix": "eslint . --fix --ignore-path .gitignore",
    "eslint": "eslint . --ignore-path .gitignore",
    "watch": "npm-watch",
    "test": "jest \"-u\"",
    "test-watch": "jest --watchAll"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./SetupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg)$": "jest-static-stubs/jpg",
      "^.+\\.gif$": "jest-static-stubs/gif",
      "^.+\\.(eot|otf|svg|png|ttf|woff|woff2|mp3|m4a|aac|oga|css|less|scss)$": "identity-obj-proxy",
      "\\.(css|less|scss|png)$": "identity-obj-proxy"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
