    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
